	<queryString>
		<![CDATA[select * from (SELECT
unid_orga.NOMB_UNID_ORGA,
pers.NOMB_PERS,
pers.APEL_PERS
FROM
visi
INNER JOIN unid_orga ON visi.CODI_UNID_ORGA_VISI = unid_orga.CODI_UNID_ORGA
INNER JOIN usua ON visi.CODI_USUA = usua.CODI_USUA
