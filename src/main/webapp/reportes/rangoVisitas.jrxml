	<queryString>
		<![CDATA[select * from (SELECT
pers.NOMB_PERS,
visi.MOTI_VISI,
visi.FECH_HORA_ENTR_VISI,
unid_orga.NOMB_UNID_ORGA
FROM
pers
INNER JOIN usua ON usua.CODI_PERS = pers.CODI_PERS
INNER JOIN visi ON visi.CODI_USUA = usua.CODI_USUA
INNER JOIN unid_orga ON visi.CODI_UNID_ORGA_VISI = unid_orga.CODI_UNID_ORGA) as intervalo
where intervalo.FECH_HORA_ENTR_VISI between $P{fei}  and $P{fef}]]>
	</queryString>
